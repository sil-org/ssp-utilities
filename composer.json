{
  "name": "silinternational/ssp-utilities",
  "description": "SimpleSAMLphp related utility classes",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Phillip Shipley",
      "email": "phillip.shipley@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.0 || ^8.1",
    "simplesamlphp/simplesamlphp": "~1.18.6 || ~1.19.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0",
    "roave/security-advisories": "dev-master",
    "satooshi/php-coveralls": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Sil\\SspUtils\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "simplesamlphp/composer-module-installer": true
    }
  }
}
