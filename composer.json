{
  "name": "silinternational/ssp-utilities",
  "description": "SimpleSAMLphp related utility classes",
  "type": "library",
  "license": "MIT",
  "version": "2.0.0",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "simplesamlphp/simplesamlphp": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Sil\\SspUtils\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "simplesamlphp/composer-module-installer": true,
      "simplesamlphp/composer-xmlprovider-installer": false
    }
  }
}
